<?xml version='1.0' encoding='utf-8'?>
<widget id="com.carloshglez.asteroids" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Asteroids</name>
    <description>
        ReeactJS Project based on "reacteroids" game
    </description>
    <author email="carlos-horacio.gonzalez-gordillo@hpe.com" href="https://github.houston.entsvcs.net/carlos-horacio-gonzalez-gordillo/react-game.git">
        carloshglez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="www/webapp/icon.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="landscape" />
    <preference name="Fullscreen" value="true" />
</widget>
